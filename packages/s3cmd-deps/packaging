set -e

DATEUTIL_VERSION=python-dateutil-2.8.0
SIX_VERSION=six-1.12.0
MAGIC_VERSION=python-magic-0.4.15

cd ${BOSH_INSTALL_TARGET}

tar xvf ${BOSH_COMPILE_TARGET}/${DATEUTIL_VERSION}.tar.gz
tar xvf ${BOSH_COMPILE_TARGET}/${SIX_VERSION}.tar.gz
tar xvf ${BOSH_COMPILE_TARGET}/${MAGIC_VERSION}.tar.gz

echo "export PYTHONPATH=$PYTHONPATH:${BOSH_INSTALL_TARGET}/${DATEUTIL_VERSION}:${BOSH_INSTALL_TARGET}/${SIX_VERSION}:${BOSH_INSTALL_TARGET}/${MAGIC_VERSION}" > ${BOSH_INSTALL_TARGET}/runtime.env

